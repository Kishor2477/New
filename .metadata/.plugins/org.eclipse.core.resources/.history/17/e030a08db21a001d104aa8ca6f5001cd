package codepractice;

public class StringReverse {

	public static void main(String[] args) {
		
	}
	public void reverseString(String str) {
		String rev = "null";
		int len = str.length();
		for(int i=len-1;i>=0;i--) {
			char charAt = str.charAt(i);
			rev = rev+charAt;
		}
		System.out.println(rev);
	}
}
